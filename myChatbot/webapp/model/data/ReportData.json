      "id": "q1",
      "title": "Osszes rendeles osszeg ugyfelenkent",
      "sql": "SELECT c.CustomerName, SUM(so.NetAmount) AS TotalAmount\nFROM Customer c\nJOIN SalesOrder so ON so.CustomerId = c.CustomerId\nGROUP BY c.CustomerName\nORDER BY TotalAmount DESC;"
    },
    {
      "id": "q2",
      "title": "Rendelesek orszagonkent",
      "sql": "SELECT c.Country, COUNT(*) AS Orders, SUM(so.NetAmount) AS TotalAmount\nFROM Customer c\nJOIN SalesOrder so ON so.CustomerId = c.CustomerId\nGROUP BY c.Country\nORDER BY TotalAmount DESC;"
    }
